{
  "name": "@atriz/core",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "migrate": "tsx migrate.ts",
    "migrate:up": "tsx migrate.ts up",
    "migrate:down": "tsx migrate.ts down",
    "migrate:create": "node-pg-migrate create --migration-file-language sql --migrations-dir migrations",
    "seed": "tsx seed.ts"
  },
  "dependencies": {
    "@atriz/database": "workspace:*",
    "bcryptjs": "^2.4.3",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "tsyringe": "^4.8.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "prettier": "^3.0.3",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vitest": "^1.0.0"
  }
}

